{{#each flashMessages.queue as |flash|}}
  {{flash-message flash=flash}}
{{/each}}

<div class='landing-info-section event-info'>

    {{event-carousel-outer events=model.event
                        slideFrom=slideFrom
                        slideTo=slideTo
                        slideClick=slideClick}}

  <div class='landing-info-wrapper index'>
    {{upcoming-region-events model=model
                            goToRegion='goToRegion'
                            goToConcert='goToConcert'
                            userChooseRegion='userChooseRegion'}}

    {{events-carousel events=model.event
                  slideFrom=slideFrom
                  slideTo=slideTo
                  slideClick=slideClick
                  goToConcert='goToConcert'}}
  </div>
</div>

{{landing-page-search-menu goToRegions='goToRegions'
                           goToArtists='goToArtists'
                           goToVenues='goToVenues'}}

{{liquid-outlet}}
