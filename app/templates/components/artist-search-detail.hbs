<li>
    {{#if artistSelect.noMatch}}
      <h3>{{artistSelect.displayName}}</h3>
    {{else}}
      {{#link-to 'artist.event.results' (query-params getId=artistSelect.id
      artist=artistSelect.displayName page=1)}}
      {{!-- {{#link-to 'artist.event.results' (query-params getId=artistSelect.id
      artist=artistSelect.displayName)}} --}}
      <div class='artist-search-wrapper'>
        <h3>{{artistSelect.displayName}}</h3>
        <figure class="band-thumb-wrapper">
          <img class="headliner image" src="{{artistSelect.imageUrl}}" alt="{{artistSelect.displayName}}" />
          <figcaption class="image-caption-text">{{artistSelect.displayName}}</figcaption>
        </figure>
      </div>
      {{/link-to}}
    {{/if}}
</li>

